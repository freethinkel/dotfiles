// ~/.config/zed/themes/nules.json
{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Nules",
  "author": "freethinkel",
  "themes": [
    {
      "name": "Nules dark",
      "appearance": "dark",
      "style": {
        "editor.background": "#1B1B1B",
        "editor.gutter.background": "#1B1B1B",
        "border": "#2B2B2B",
        "panel.background": "#181818",
        "panel.focused_border": "#ff0000",
        "tab_bar.background": "#1B1B1B",
        "tab.active_background": "#181818",
        "tab.inactive_background": "#1B1B1B",
        "toolbar.background": "#181818",
        "text.accent": "#75FB9F",
        "icon.accent": "#75FB9F",
        "title_bar.background": "#181818",

        "pane.focused_border": "#ff0000",
        "border.transparent": "#0000ff",
        "border.selected": "#ff0000",
        "element.selected": "#FFFEAA40",
        "border.focused": "#ff000",
        "created": "#A1FFC1",
        "modified": "#ffc799",
        "conflict": "#d8a8ff",
        "deleted": "#ff8080",
        "scrollbar.track.border": "#00000000",
        "search.match_background": "#ffc79930",

        "text.muted": "#A8A29F",
        "ignored": "#7C7C7C",
        "hint": "#5F5F5F",
        "editor.active_line_number": "#ffffff",
        "editor.active_line.background": "#262626",

        "terminal.background": "#181818",
        "terminal.foreground": "#ffffff",
        "terminal.dim_foreground": "#ffffff",
        "terminal.bright_foreground": "#ffffff",
        "terminal.ansi.black": "#353535",
        "terminal.ansi.dim_black": "#353535",
        "terminal.ansi.bright_black": "#353535",
        "terminal.ansi.red": "#ff8080",
        "terminal.ansi.dim_red": "#ff8080",
        "terminal.ansi.bright_red": "#ff8080",
        "terminal.ansi.green": "#A1FFC1",
        "terminal.ansi.dim_green": "#A1FFC1",
        "terminal.ansi.bright_green": "#A1FFC1",
        "terminal.ansi.yellow": "#ffc799",
        "terminal.ansi.dim_yellow": "#ffc799",
        "terminal.ansi.bright_yellow": "#ffc799",
        "terminal.ansi.blue": "#A9F0FD",
        "terminal.ansi.dim_blue": "#A9F0FD",
        "terminal.ansi.bright_blue": "#A9F0FD",
        "terminal.ansi.magenta": "#d8a8ff",
        "terminal.ansi.dim_magenta": "#d8a8ff",
        "terminal.ansi.bright_magenta": "#d8a8ff",
        "terminal.ansi.cyan": "#87f2ff",
        "terminal.ansi.dim_cyan": "#87f2ff",
        "terminal.ansi.bright_cyan": "#87f2ff",
        "terminal.ansi.white": "#bbbbbb",
        "terminal.ansi.dim_white": "#bbbbbb",
        "terminal.ansi.bright_white": "#bbbbbb",

        "drop_target.background": "#75FB9F20",
        "elevated_surface.background": "#262626",
        "surface.background": "#262626",
        "text": "#ffffff",
        "predictive": "#3C3C3C",

        // "editor.document_highlight.read_background": "#00ff0040",
        // "editor.document_highlight.write_background": "#0000ff",
        // "editor.highlighted_line.background": "#ff0000",
        "warning": "#ffc799",
        "warning.border": "#ffc79920",
        "warning.background": "#38312B",
        "error": "#ff8080",
        "error.border": "#ff808020",
        "error.background": "#352525",
        "info": "#A9F0FD",
        "info.border": "#A9F0FD",
        "info.background": "#3C4F50",

        "players": [
          {
            "cursor": "#75FB9Ff2",
            "background": "#75FB9Ff2",
            "selection": "#75FB9F40"
          }
        ],
        "syntax": {
          "comment": {
            "color": "#5C5C5C"
          },
          "comment.doc": {
            "color": "#666666",
            "font_style": "italic",
            "font_weight": null
          },
          "function": {
            "color": "#9FECB7",
            "font_style": null,
            "font_weight": null
          },
          "keyword": {
            "color": "#A0A0A0",
            "font_style": null
          },
          "operator": {
            "color": "#A0A0A0",
            "font_style": null
          },
          "punctuation": {
            "color": "#333333",
            "font_style": null,
            "font_weight": 700
          },
          "punctuation.bracket": {
            "color": "#75FB9F",
            "font_style": null
          },
          "punctuation.delimiter": {
            "color": "#5b5b5b",
            "font_style": null,
            "font_weight": 700
          },
          "punctuation.list_marker": {
            "color": "#ffc799",
            "font_style": null,
            "font_weight": 700
          },
          "punctuation.special": {
            "color": "#5b5b5b",
            "font_style": null
            // "font_weight": 700
          },
          "string": {
            "color": "#FFFEAA",
            "font_style": null
          },
          "type": {
            "color": "#9FECB7",
            "font_style": null,
            "font_weight": null
          },
          "tag": {
            "color": "#9FECB7",
            "font_style": null
          },
          "property": {
            "color": "#ffffff"
          },
          "attribute": {
            "color": "#9FECB7"
          },
          "variable": {
            "color": "#ffffff"
          },
          "variable.parameter": {
            "color": "#ffffff"
          }
        }
      }
    }
  ]
}
