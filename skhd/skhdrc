#SKHD STUFF

ctrl + alt + cmd - y : /opt/homebrew/bin/brew services restart yabai
ctrl + alt + cmd - s : /opt/homebrew/bin/brew services restart skhd

## open terminal
# ctrl - return : /Applications/kitty.app/Contents/MacOS/kitty ~
ctrl - return : /Applications/Alacritty.app/Contents/MacOS/alacritty

rcmd + alt - space : yabai -m window --toggle float 
rcmd + alt - c : yabai -m window --toggle float; yabai -m window --grid 1:6:1:1:4:1
rcmd + alt - f : yabai -m window --toggle zoom-fullscreen

rcmd - 0x2B : ~/Developer/dev/dotfiles/bin/rcmd toggle_layout

rcmd - a : ~/Developer/dev/dotfiles/bin/rcmd a
rcmd - b : ~/Developer/dev/dotfiles/bin/rcmd b
rcmd - c : ~/Developer/dev/dotfiles/bin/rcmd c
rcmd - f : ~/Developer/dev/dotfiles/bin/rcmd f
rcmd - g : ~/Developer/dev/dotfiles/bin/rcmd g
rcmd - k : ~/Developer/dev/dotfiles/bin/rcmd k
rcmd - t : ~/Developer/dev/dotfiles/bin/rcmd t
rcmd - s : ~/Developer/dev/dotfiles/bin/rcmd s
rcmd - v : ~/Developer/dev/dotfiles/bin/rcmd v
rcmd - z : ~/Developer/dev/dotfiles/bin/rcmd z

rcmd - 1 : yabai -m space --focus 1
rcmd - 2 : yabai -m space --focus 2
rcmd - 3 : yabai -m space --focus 3
rcmd - 4 : yabai -m space --focus 4
rcmd - 5 : yabai -m space --focus 5
rcmd - 6 : yabai -m space --focus 6
rcmd - 7 : yabai -m space --focus 7
rcmd - 8 : yabai -m space --focus 8
