#SKHD STUFF

ctrl + alt + cmd - r : launchctl kickstart -k "gui/${UID}/homebrew.mxcl.yabai"
ctrl + alt + cmd - s : launchctl kickstart -k "gui/${UID}/homebrew.mxcl.skhd"

## open terminal
ctrl - return : alacritty

ctrl + shift - space : yabai -m window --toggle float && yabai -m window --toggle topmost && yabai -m window --layer above

ctrl - space : ~/projects/dev/dotfiles/bin/popup.sh ~/projects/dev/dotfiles/bin/launcher.sh

ctrl + alt - f : yabai -m window --toggle zoom-fullscreen
